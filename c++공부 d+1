#include <iostream>

int main(void)
{
	int val1, val2;
	int result=0;
	std::cout<<"두 개의 숫자 입력 : ";
	std::cin>>val1>>val2;
	
	if (val1 < val2)
	{
		for (int i=val1+1; i<val2; i++)
		{
			result+=i;	
		}
	}
	else
	{
		for (int i=val2+1; i<val1; i++)
		{
			result+=i;
		}
	}
	std::cout<<"두 수 사이의 정수 합 : "<<result<<std::endl;
		return 0;
}


---------------------------------------------------------
#include <iostream>

int main(void)
{
	char name[100];
	char lang[200];
	
	std::cout<<"이름은 무엇입니까? :";
	std::cin>>name;
	
	std::cout<<"좋아하는 프로그래밍 언어는 무엇인가요? ";
	std::cin>>lang;
	
	std::cout<<"내 이름은 "<<name<<"입니다.\n";
	std::cout<<"제일 좋아하는 언어는 "<<lang<<"입니다."<<std::endl;
	return 0;
}

----------------------------------------------------------<문제1>
#include <iostream>

int main(void)
{
	int val1, val2, val3, val4, val5, result=0 ;
	std::cout<<"1번째 정수 입력 : ";
	std::cin>>val1;
	std::cout<<"2번째 정수 입력 : ";
	std::cin>>val2;
	std::cout<<"3번째 정수 입력 : ";
	std::cin>>val3;
	std::cout<<"4번째 정수 입력 : ";
	std::cin>>val4;
	std::cout<<"5번째 정수 입력 : ";
	std::cin>>val5;
		
	result = val1 + val2 + val3 + val4 + val5;
	
	std::cout<<"합계 : "<<result<<std::endl;
	return 0;
}

----------------------------------------------------------<문제3>
#include <iostream>

int main(void)
{
	int n;
	std::cout<<"1부터 9까지의 숫자중 하나를 입력하세요 : ";
	std::cin>>n;
	
	for(int i = 1 ; i<10 ; i++)
	{
		std::cout<<n<<" X "<<i<<" = "<<n*i<<std::endl;
	}
	return 0;
}

----------------------------------------------------------<문제3>
